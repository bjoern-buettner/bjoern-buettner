<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    RewriteRule ^index\.php$ - [L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . index.php [L]
</IfModule>
AuthType Basic
AuthName "Bjoern-Buettner.me - Private"
AuthUserFile /var/bjoern-buettner.me/public/.htpasswd
require valid-user